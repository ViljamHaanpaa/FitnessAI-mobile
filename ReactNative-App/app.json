{
  "expo": {
    "name": "FitLane",
    "slug": "fittie",
    "version": "0.6.2",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "fittie",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.fittie.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#011223"
        }
      ],
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "owner": "fittie",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "4e62e45d-6201-4ebb-bc17-31f7695eec72"
      }
    }
  }
}
